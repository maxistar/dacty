// layers
#define DEFAULT 0
#define DEFAULT_ANDROID 1
#define DEUTSCH 2
#define DE_MAC  3
#define RUSSIAN 4
#define RUSSIAN_MAC 5
#define RU_ANDROID 6
#define LOWER 7
#define MAGIC 8
#define DE_SHIFT 9
#define DE_SHIFT_MAC 10
#define DE_RALT 11
#define DE_RALT_MAC 12
#define RU_SHIFT 13
#define RU_SHIFT_MAC 14

/ {
    combos {
        compatible = "zmk,combos";
        combo_esc {
            timeout-ms = <50>;
            key-positions = <1 2>;
            bindings = <&kp ESC>;
        };
        combo_enter {
            timeout-ms = <50>;
            key-positions = <31 32 33>;
            bindings = <&kp RET>;
       };
    };
};

/ {
    macros {

        to_linux: to_linux {
          compatible = "zmk,behavior-macro";
          #binding-cells = <0>;
          bindings = <&bt BT_SEL 0 &to DEUTSCH>;
        };

        to_mac: to_mac {
          compatible = "zmk,behavior-macro";
          #binding-cells = <0>;
          bindings = <&bt BT_SEL 1>,
                     <&to DE_MAC>;
        };

        to_android: to_android {
          compatible = "zmk,behavior-macro";
          #binding-cells = <0>;
          bindings = <&bt BT_SEL 2>,
                     <&to DEFAULT_ANDROID>;
        };

        cycle_lang_de: cycle_lang_de {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&to DEUTSCH>,
                  <&kp CAPS>;
        };

        cycle_android: cycle_android {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&to RU_ANDROID>,
                  <&sk LCTRL &kp SPACE>;
        };

        cycle_android_de: cycle_android_de {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&to DEFAULT_ANDROID>,
